{
  "version": 2,
  "env": {
    "DJANGO_SETTINGS_MODULE": "ecommerce.vercel_settings",
    "VERCEL": "1",
    "PYTHONUNBUFFERED": "1"
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "ecommerce.vercel_settings",
      "VERCEL": "1",
      "PYTHONUNBUFFERED": "1"
    }
  },
  "builds": [
    {
      "src": "ecommerce/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ecommerce/wsgi.py"
    }
  ],
  "buildCommand": "",
  
  "devCommand": "python manage.py runserver",
  "framework": null
}
