{
  "version": 2,
  "builds": [
    { "src": "ecommerce/wsgi.py", "use": "@vercel/python", "config": { "excludeFiles": ["media/**", "static/**", "staticfiles/**", "ecommerce.zip", "pbl/**", "docs/**", "tests/**", "logs/**", "t-shirt-designer-webapp/**"], "includeFiles": ["ecommerce/**", "templates/**", "manage.py", "requirements.txt", "ecommerce/vercel_settings.py"] } },
    { "src": "static/**/*", "use": "@vercel/static" },
    { "src": "staticfiles/**/*", "use": "@vercel/static" },
    { "src": "media/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "static/$1" },
    { "src": "/staticfiles/(.*)", "dest": "staticfiles/$1" },
    { "src": "/media/(.*)", "dest": "media/$1" },
    { "src": "/(.*)", "dest": "ecommerce/wsgi.py" }
  ]
}
